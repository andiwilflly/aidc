<div class="reporting__period">
	<p class="reporting__period-title">
		Compare activities by Markets:
		{{ reportModel.activityName }}
		{{#if reportModel.marketName }}
			| {{ reportModel.marketName }}
		{{/if}}
	</p>
	<p class="reporting__period-value">
		<span><b>From: </b>w{{ getWeek reportModel.requestObj.startDate }}  {{ dateReplaceDotOnSlash reportModel.requestObj.startDate }}</span>
		<span><b>To:   </b>w{{ getWeek reportModel.requestObj.endDate }} {{ dateReplaceDotOnSlash reportModel.requestObj.endDate }}</span>
	</p>
</div>
